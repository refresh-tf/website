@font-face {
    font-family: "{{fontName}}";
    src: {{{src}}};
    font-display: swap;
}

{{baseSelector}} {
    -moz-osx-font-smoothing: grayscale;
    -webkit-font-smoothing: antialiased;
    line-height: 1;
}

{{baseSelector}}:before,
{{baseSelector}}:after {
    font-family: {{fontName}} !important;
    font-style: normal;
    font-weight: normal !important;
    vertical-align: top;
    display: inline-block;
}

{{#each codepoints}}
.mdi-{{@key}}:before {
    content: "\\{{this}}";
}
{{/each}}
{{#each codepoints}}
.mdib-{{@key}}:before,
.mdia-{{@key}}:after {
    content: "\\{{this}}";
}
{{/each}}
